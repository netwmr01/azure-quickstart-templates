{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.1",
  "parameters": {
    "resourceAPIVersion":{
      "type":"string"
    },
    "vmName":{
      "type":"string"
    },
    "clusterParameters":{
      "type":"secureObject"
    }
  },
  "resources": [
    {
      "type": "Microsoft.Compute/virtualMachines/extensions",
      "name": "[concat(parameters('vmName'), '/prepareDisks')]",
      "apiVersion": "[parameters('resourceAPIVersion')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "publisher": "Microsoft.OSTCExtensions",
        "type": "CustomScriptForLinux",
        "typeHandlerVersion": "1.4",
        "autoUpgradeMinorVersion": true,
        "settings": {
          "commandToExecute": "[concat('python prepare-director-conf.py \"', parameters('clusterParameters'), '\" \"', parameters('vmSpec').dbUserName, '\" \"', parameters('vmSpec').dbPassword,'\"')]"

          "commandToExecute": "python prepare-director-conf.py mytest eastus sub_id, ten_id client_id client_secret user_name mykey networkSecurityGroupResourceGroup networkSecruityGroup virtualNetworkResourceGroup virtualNetwork subnetName computeResourceGroup hostFqdnSuffix 10.0.0.1 dbUserName dbPassword ds13 ds14 ds13"
        }
      }
    }
  ]
}
